/// Simple HashMap OOP API demonstration
/// Shows basic usage of HashMap with method calls

use collections.hashmap

fn main() int {
    let map = HashMap.new()

    // Insert key-value pairs
    map.insert_str("name", "Alice")
    map.insert_str("city", "Wonderland")
    map.insert_int("age", 25)

    // Retrieve values
    let name = map.get_str("name")
    let age = map.get_int("age")

    // Check containment
    let has_name = map.contains("name")
    let has_missing = map.contains("missing")

    // Get size
    let size = map.size()

    // Print results (for manual verification)
    printf(c"Name: %s\n", name)
    printf(c"Age: %d\n", age)
    printf(c"Has name: %d\n", has_name)
    printf(c"Has missing: %d\n", has_missing)
    printf(c"Size: %zu\n", size)

    // Clean up
    map.drop()

    // Return success
    0
}
