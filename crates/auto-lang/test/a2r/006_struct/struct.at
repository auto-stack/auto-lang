type Point {
    x int
    y int
}

type Circle {
    radius float
    border uint
    center Point
}

fn main() {
    mut p = Point(1, 2)
    p.x = 3
    print("P: ", p.x, ", ", p.y)

    let circle = Circle(5.0, 1, Point(50, 50))
    print("C: ", circle.center.x, ", ", circle.center.y, ", ", circle.radius)
}
