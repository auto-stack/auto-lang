fn test_hashmap_oop() int {
    // Create a HashMap
    let map = HashMap_new()

    // Insert some string values
    HashMap_insert_str(map, "name", "Alice")
    HashMap_insert_str(map, "city", "Wonderland")

    // Get values
    let name = HashMap_get_str(map, "name")
    let city = HashMap_get_str(map, "city")

    // Check contains
    let has_name = HashMap_contains(map, "name")
    let has_age = HashMap_contains(map, "age")

    // Get size
    let size = HashMap_size(map)

    // Test with int values
    HashMap_insert_int(map, "count", 42)
    let count = HashMap_get_int(map, "count")

    // Cleanup
    HashMap_drop(map)

    count
}

fn main() {
    test_hashmap_oop()
}
