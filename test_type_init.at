type Point {
    x int = 10
    y int = 20
    name str = "point"
}

fn main() {
    println("Starting test...")
    let p Point
    println("p created")
    
    if p.x == 10 {
        println("p.x check passed")
    } else {
        println("Error: p.x should be 10, got " + p.x)
    }

    if p.y == 20 {
        println("p.y check passed")
    } else {
        println("Error: p.y should be 20, got " + p.y)
    }
    
    if p.name == "point" {
        println("p.name check passed")
    } else {
         println("Error: p.name should be 'point', got '" + p.name + "'")
    }

    // Test nested init
    p.x = 100
    if p.x == 100 {
        println("p.x assignment passed")
    } else {
        println("Error: p.x assignment failed")
    }
    
    println("Type initialization test finished!")
}
