/// Static string
type sstr {
    size usize
    data [size]char

    fn print() {
        for ch in .data[0...size] {
            print(ch)
        }
    }
}

/// dynamic string
type dstr {
    size usize
    data []char

    fn print() {
        for ch in .data[0...size] {

        }

    }
}

/// view string / virtual string / slice
type vstr {
    size usize
    data *char
}

