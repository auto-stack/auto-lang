type File {
    #[pub]
    path str

    #[pub]
    static fn open(path str) File;

    #[pub]
    fn read_text() str;

    #[pub]
    fn read_line() str;

    #[pub]
    fn read_char() int;

    #[pub]
    fn read_buf(buf str, size int) int;

    #[pub]
    fn write_line(s str);

    #[pub]
    fn flush();

    #[pub]
    fn close();
}

#[pub]
fn say(msg str);