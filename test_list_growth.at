// Test List automatic capacity expansion (Plan 052)
// Verifies that List.push() automatically grows beyond initial capacity

fn main() {
    let list = List.new()
    
    // Push many elements to force multiple reallocations
    // Rust Vec starts with 0, then grows to 1, 2, 4, 8, 16, etc.
    for i in 0..100 {
        list.push(i)
    }
    
    let len = list.len()
    let cap = list.capacity()
    
    print("Length:", len)
    print("Capacity:", cap)
    
    // Verify elements are correct
    let tenth = list.get(10)
    let hundredth = list.get(99)
    
    print("10th element:", tenth)
    print("100th element:", hundredth)
}
