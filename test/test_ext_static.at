// Test instance methods in ext statement (Plan 035 Phase 4)

ext int {
    fn double() int {
        self + self
    }

    fn triple() int {
        self + self + self
    }
}

fn main() {
    let x = 5
    let doubled = x.double()
    if doubled == 10 {
        print("PASS: double() works")
    } else {
        print("FAIL: double() returned " + doubled)
    }

    let y = 7
    let tripled = y.triple()
    if tripled == 21 {
        print("PASS: triple() works")
    } else {
        print("FAIL: triple() returned " + tripled)
    }
}
